  "scripts": {
    "build": "npm run prisma:generate && nest build",
    "postinstall": "npm run prisma:generate",
    "prisma:generate": "npx prisma generate --schema=prisma/postgre/schema.prisma && npx prisma generate --schema=prisma/mongo/schema.prisma",
    "prisma:generate:postgre": "npx prisma generate --schema=prisma/postgre/schema.prisma",
    "prisma:generate:mongo": "npx prisma generate --schema=prisma/mongo/schema.prisma",
    "prisma:migrate:postgre": "npx prisma migrate deploy --schema=prisma/postgre/schema.prisma",
    "prisma:push:mongo": "npx prisma db push --schema=prisma/mongo/schema.prisma",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
